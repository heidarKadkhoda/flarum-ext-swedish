{
    "name": "heidar/flarum-ext-swedish",
    "type": "flarum-extension",
    "description": "Swedish language pack.",
    "keywords": ["locale"],
    "license": "MIT",
    "authors": [
        {
            "name": "Heidar Kadkhoda",
            "email": "heidar.kadkhoda@gmail.com"
        }
    ],

    "support": {
        "issues": "https://github.com/heidarKadkhoda/flarum-ext-swedish/issues",
        "source": "https://github.com/heidarKadkhoda/flarum-ext-swedish"
    },

    "require": {
        "flarum/core": "^0.1.0-beta.7"
    },

    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        },

        "flarum-extension": {
            "title": "Svenska",
            "icon": {
                "image": "icon.svg",
                "backgroundColor": "#00247d",
                "backgroundSize": "cover",
                "backgroundPosition": "center"
            }
        },

        "flarum-locale": {
            "code": "sv",
            "title": "Svenska"
        }
    }
}
